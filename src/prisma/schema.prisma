datasource db {
    provider = "mongodb"
    url      = env("DB_URL")
}

generator client {
    provider = "prisma-client-js"
}

type ClassName {
    name String
}

model Student {
    id        String      @id @default(auto()) @map("_id") @db.ObjectId
    firstName String
    lastName  String
    email     String      @unique
    className ClassName[]
    createdAt DateTime    @default(now())
    updatedAt DateTime    @updatedAt
}
